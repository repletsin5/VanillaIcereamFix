{
  "package": "org.dimdev.vanillafix.crashes.mixins",
  "required": true,
  "refmap": "mixins.vanillafix.refmap.json",
  "target": "@env(DEFAULT)",
  "minVersion": "0.6",
  "plugin": "org.dimdev.vanillafix.ModCompatibilityMixinPlugin",
  "compatibilityLevel": "JAVA_8",
  "mixins": [
    "MixinCrashReport",
    "MixinIntegratedServer",
    "MixinUtil",
    "MixinCrashReportCategory",
    "MixinEntity",
    "MixinTileEntity"
  ],
  "client": [
    "client.MixinMinecraft",
    "client.MixinBufferBuilder",
    "client.MixinTileEntityRenderDispatcher"
  ],
  "injectors": {
    "maxShiftBy": 10
  }
}
